#include <stdio.h>

int main()
{
  int a,b,c;
  printf("enter 3 values for a,b,c\n");
  scanf("%d%d%d",&a,&b,&c);
  if(a<b&&a<c)
  {
      printf("the smallest is %d",a);
  }
  else if(b<c&&b<a)
  {
      printf("the smallest is %d",b);
  }
  else if(c<b&&c<a)
  {
      printf("the smallest is %d",c);
  }
  return 0;
}
 
